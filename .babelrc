{
  "passPerPreset": true,
  "presets": [
    "react", "es2015", "stage-0"
  ],
  "plugins": [
    "./server/babelRelayPlugin",
    "transform-decorators-legacy",
    [
      "react-transform",
      {
        "transforms": [
          {
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }
        ]
      }
    ]
  ]
}
